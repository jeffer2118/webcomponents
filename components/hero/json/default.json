{
    "cardStyle": {
        "vertical": {
            "default":{
                ".card": {
                    "color": "#1b1b1b",
                    "background-color": "#fff",
                    "font-family": "Source Sans Pro Web,Helvetica Neue,Helvetica,Roboto,Arial,sans-serif",
                    "font-size": "1.06rem",
                    "line-height": "1.5",
                    "border-width": "2px",
                    "border-color": "#e6e6e6",
                    "border-style": "solid",
                    "display": "flex",
                    "height": "100%",
                    "flex-direction": "column",
                    "margin-left": "0.5rem",
                    "margin-right": "0.5rem",
                    "position": "relative",
                    "border-radius": "0.25rem"
                },
                ".header": {
                    "padding": "1.5rem 1.5rem .5rem"
                },
                "h2": {
                    "font-family": "Merriweather Web,Georgia,Cambria,Times New Roman,Times,serif",
                    "font-size": "1.22rem",
                    "line-height": "1.2",
                    "margin": "0"
                },
                ".body": {
                    "flex": "1 1 0%",
                    "padding": "0.5rem 1.5rem",
                    "flex-basis": "auto"
                },
                ".footer": {
                    "padding": ".5rem 1.5rem 1.5rem"
                },
                ".media": {
                    "order": "-1"
                },
                "img": {
                    "display": "block",
                    "height": "100%",
                    "width": "100%",
                    "object-fit": "cover"
                }
            }
        },
        "horizontal": {
            "default":{
                ".card": {
                    "color": "#1b1b1b",
                    "background-color": "#fff",
                    "font-family": "Source Sans Pro Web,Helvetica Neue,Helvetica,Roboto,Arial,sans-serif",
                    "font-size": "1.06rem",
                    "line-height": "1.5",
                    "border-width": "2px",
                    "border-color": "#e6e6e6",
                    "border-style": "solid",
                    "display": "flex",
                    "height": "100%",
                    "flex-direction": "column",
                    "margin-left": "0.5rem",
                    "margin-right": "0.5rem",
                    "position": "relative",
                    "border-radius": "0.25rem"
                },
                ".header": {
                    "padding": "1.5rem 1.5rem .5rem",
                    "margin-left": "15rem"
                },
                "h2": {
                    "font-family": "Merriweather Web,Georgia,Cambria,Times New Roman,Times,serif",
                    "font-size": "1.22rem",
                    "line-height": "1.2",
                    "margin": "0"
                },
                ".body": {
                    "flex": "1 1 0%",
                    "padding": "0.5rem 1.5rem",
                    "flex-basis": "auto",
                    "margin-left": "15rem"
                },
                ".footer": {
                    "padding": ".5rem 1.5rem 1.5rem",
                    "margin-left": "15rem"
                },
                ".media": {
                    "order": "-1",
                    "bottom": "0",
                    "top": "0",
                    "left": "0",
                    "position": "absolute",
                    "overflow": "hidden",
                    "width": "15rem",
                    "display": "flex"
                },
                ".img": {
                    "border-radius": "0",
                    "border-top-left-radius": "calc(0.25rem - 2px)",
                    "border-bottom-left-radius": "calc(0.25rem - 2px)",
                    "background-color": "#f0f0f0",
                    "position": "relative",
                    "overflow": "hidden"
                },
                "img": {
                    "display": "block",
                    "height": "100%",
                    "width": "100%",
                    "object-fit": "cover"
                }
            }
        }
    },
